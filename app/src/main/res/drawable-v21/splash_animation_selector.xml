<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/init"
        android:state_activated="true">
        <bitmap
            android:src="@drawable/alternative3_000"/>
    </item>

    <item
        android:id="@+id/end">
        <bitmap
            android:src="@drawable/alternative3_013"/>
    </item>

    <transition
        android:fromId="@id/init"
        android:toId="@id/end"
        android:drawable="@drawable/splash_animation_list">
    </transition>

</animated-selector>